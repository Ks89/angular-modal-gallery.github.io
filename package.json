    "codemirror": "^5.25.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.0",
    "ng2-codemirror": "^1.1.1",
    "ng2-page-scroll": "^4.0.0-beta.5",
