    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "codelyzer",
      "@types/core-js",
      "@types/jasmine",
      "@types/jquery",
